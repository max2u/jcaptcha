<deployment xmlns="http://xml.apache.org/axis/wsdd/"
            xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">
 <service name="ImageCaptchaService" provider="java:RPC">
  <parameter name="className" value="com.octo.captcha.j2ee.rpc.ImageCaptchaWebServiceEndpoint"/>
  <parameter name="allowedMethods" value="generateCaptchaAndRenderChallengeAsJpeg verifyResponseToACaptchaChallenge"/>
  <parameter name="scope" value="application"/>
 </service>
</deployment>